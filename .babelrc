{
  "env": {
    "development": {
      "presets": ["es2015-rollup", "react"],
      "plugins": [
        "transform-class-properties"
      ]
    },
    "production": {
      "presets": ["es2015-rollup", "react"],
      "plugins": [
        "transform-class-properties"
      ]
    },
    "test": {
      "presets": [
    		["es2015", { "modules": false }],
    		"react"
    	],
			"plugins": [
        "transform-class-properties",
				"transform-es2015-modules-commonjs"
			]
    }
  }
}
